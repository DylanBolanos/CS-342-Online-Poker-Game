<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="clientEndScreenController"
            prefHeight="1000.0" prefWidth="700.0"
            fx:id="root">
    <center>
        <VBox maxHeight="400" maxWidth="500" >
            <TextField fx:id="messageText" editable="false"/>
            <Button fx:id="playAgain" text="Play Again" onAction="#playAgain" alignment="CENTER_RIGHT"/>
            <Button fx:id="quitGame" text="Quit Game"  onAction="#quitGame" alignment="CENTER"/>
            <TextField fx:id="amountWonText" editable="false"/>
        </VBox>
    </center>


</BorderPane>
